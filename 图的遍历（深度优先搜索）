/*
图的遍历（深度优先搜索）
用二维数组来表示图
0表示自己到自己或没有 
1表示i到j有边
*/

#include<stdio.h>

int a[50][50], book[50]; //book数组用于记录结点是否被走过
int sum; //记录结点个数变化
int n; //结点总个数 

void dfs(int cur) { //cur当前所在结点编号 
    int i; 
	 
	printf("%d", cur);
	sum++;
	if(sum == n) {
		return; //所有节点都已走过，直接退出 
	} 
	for(i = 0; i < n; i++) {
		if(a[cur][i] == 1 && book[i] == 0) {
			book[i] = 1; //标记i顶点已访问过 
			dfs(i); //从i顶点开始继续遍历 
		}
	}
	return; 
} 

int main(void) {
	int i, j;
	
	//录入图的信息 
	scanf("%d", &n);
	for(i = 0; i < n; i++) {
		for(j = 0; j < n; j++) {
			scanf("%d", &a[i][j]);
		}
	}
	
	book[0] = 1;
	dfs(0);
	
	return 0;
}
